 <script type="text/javascript">

var w = 400,
    h = 300,
    padding = 10;
      
d3.csv('test.csv', function(error, data) {

  if (error) { 
    console.log(error);
  } else { 
    dataset = data;
    generateVis(dataset);
  }
  });

function generateVis(dataset){

 //var data = [1, 2, 2, 3, 5, 8];
 var data = dataset;
  var padding = 4;

var margin = {top: 40, right: 40, bottom: 40, left: 40},
    width = 960,
    height = 500;

var x = d3.scale.linear()
    .domain([0, d3.max(data, function(d){return d.Art})])
    .range([0, width - margin.left - margin.right]);

var y = d3.scale.linear()
    .domain([data.length, 0])
    .range([0, height - margin.top - margin.bottom]);

//Define X axis
var xAxis = d3.svg.axis()
    .scale(x)
    .orient("bottom")
    .tickPadding(8);

//Define Y axis
var yAxis = d3.svg.axis()
    .scale(y)
    .orient("left")
    .ticks(8);


var svg = d3.select("body").append("svg")
    .attr("width", width)
    .attr("height", height)
    .attr("class", "bar chart")
  .append("g")
    .attr("transform", "translate(" + margin.left + "," + margin.top + ")");

svg.selectAll(".bar")
    .data(data)
  .enter().append("rect")
    .attr("class", "bar")
    .attr("y", function(d, i) { return y(i); })
    .attr("width", function(d){return x(d.Art)})
    .attr("height", 10);

  //Attach X Axis
  svg.append("g")
    .attr("class", "axis")
    .attr("transform", "translate(0," + (height - margin.top - margin.bottom) + ")")
    .call(xAxis);

  //Attach Y axis
  svg.append("g")
    .attr("class", "axis")
    .attr("transform", "translate(" + padding + ",0)")
    .call(yAxis);
}//End of CSV



  </script>

 

